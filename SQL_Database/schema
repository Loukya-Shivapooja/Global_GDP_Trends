CREATE TABLE health_exp (
    Country_Name VARCHAR(40) NOT NULL,
    Country_Code VARCHAR(3) NOT NULL,
    2001_ INT NOT NULL,
    2002_ INT NOT NULL,
    2003_ INT NOT NULL,
    2004_ INT NOT NULL,
    2005_ INT NOT NULL,
    2006_ INT NOT NULL,
    2007_ INT NOT NULL,
    2008_ INT NOT NULL,
    2009_ INT NOT NULL,
    2010_ INT NOT NULL,
    2011_ INT NOT NULL,
    2012_ INT NOT NULL,
    2013_ INT NOT NULL,
    2014_ INT NOT NULL,
    2015_ INT NOT NULL,
    2016_ INT NOT NULL,
    2017_ INT NOT NULL,
    2018_ INT NOT NULL,
    2019_ INT NOT NULL,
    Income_Group VARCHAR(30) NOT NULL,
    PRIMARY KEY (Country_Code)
);

CREATE TABLE exports (
    Country_Name VARCHAR(40) NOT NULL,
    Country_Code VARCHAR(3) NOT NULL,
    2001_ INT NOT NULL,
    2002_ INT NOT NULL,
    2003_ INT NOT NULL,
    2004_ INT NOT NULL,
    2005_ INT NOT NULL,
    2006_ INT NOT NULL,
    2007_ INT NOT NULL,
    2008_ INT NOT NULL,
    2009_ INT NOT NULL,
    2010_ INT NOT NULL,
    2011_ INT NOT NULL,
    2012_ INT NOT NULL,
    2013_ INT NOT NULL,
    2014_ INT NOT NULL,
    2015_ INT NOT NULL,
    2016_ INT NOT NULL,
    2017_ INT NOT NULL,
    2018_ INT NOT NULL,
    2019_ INT NOT NULL,
    Income_Group VARCHAR(30) NOT NULL,
    PRIMARY KEY (Country_Code)
);

CREATE TABLE consumption (
    Country_Name VARCHAR(40) NOT NULL,
    Country_Code VARCHAR(3) NOT NULL,
    2001_ INT NOT NULL,
    2002_ INT NOT NULL,
    2003_ INT NOT NULL,
    2004_ INT NOT NULL,
    2005_ INT NOT NULL,
    2006_ INT NOT NULL,
    2007_ INT NOT NULL,
    2008_ INT NOT NULL,
    2009_ INT NOT NULL,
    2010_ INT NOT NULL,
    2011_ INT NOT NULL,
    2012_ INT NOT NULL,
    2013_ INT NOT NULL,
    2014_ INT NOT NULL,
    2015_ INT NOT NULL,
    2016_ INT NOT NULL,
    2017_ INT NOT NULL,
    2018_ INT NOT NULL,
    2019_ INT NOT NULL,
    Income_Group VARCHAR(30) NOT NULL,
    PRIMARY KEY (Country_Code)
)
